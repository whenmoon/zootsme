    "setupFilesAfterEnv": [
      "./setup-tests.js"
    ]
  },
  "dependencies": {
    "axios": "^0.19.0",
    "expo": "^33.0.0",
    "firebase": "^6.1.1",
    "jest-enzyme": "^7.0.2",
    "react": "16.8.3",
    "react-dom": "^16.8.6",
    "react-native": "^0.59.9",
    "react-native-dotenv": "^0.2.0",
    "react-native-mock-render": "^0.1.4",
    "react-native-router-flux": "^4.0.6",
    "react-native-scripts": "^2.0.1",
    "react-native-testing-library": "^1.9.1",
    "react-native-web": "^0.11.4",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "babel-preset-expo": "^5.1.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "jest": "^24.8.0",
    "jest-expo": "^33.0.2",
    "react-test-renderer": "^16.8.6"
  },
